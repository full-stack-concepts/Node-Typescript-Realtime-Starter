{
    "name": "angular-react-nodejs-starter",
    "version": "0.0.1",
    "description": "A starting point for Node.js express apps with TypeScript, embedded clients and data generator.",
    "repository": {
        "type": "git",
        "url": "https://github.com/tcaabv/Node-Typescript-Realtime-Starter"
    },
    "author": "Paul Vermeer",
    "license": "MIT",
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ]
    },
    "nyc": {
        "check-coverage": false,
        "extension": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
        ],
        "reporter": [
            "html",
            "text",
            "text-summary"
        ],
        "report-dir": "./coverage"
    },
    "scripts": {
        "start": "npm run serve",
        "build": "concurrently -k -n \"npm run delete-dist\" \"npm run build-ts\"",
        "serve": "node ./dist/index.js",
        "watch": "cross-env NODE_ENV=dev concurrently -k -p \"[{name}]\" -n \"npm run delete-dist\" \"npm run watch-ts\" \"npm run watch-node\"",
        "watch2": "concurrently -k -p \"[{name}]\" -n \"npm run delete-dist\" \"npm run watch-ts\" \"npm run watch-node\"",
        "watch-node": "nodemon dist/src/index.js",
        "test": "jest ./tests/user/ --forceExit --coverage --verbose",
        "unit-encryption-tests": "nyc mocha -r ts-node/register tests/unit/encryption/**/*.spec.ts --timeout=5000 --exit",
        "unit-env-secrets-tests": "mocha -r ts-node/register tests/unit/secrets/**/*.spec.ts",
        "unit-infrastructure-tests": "mocha -r ts-node/register tests/unit/infrastructure/**/*.spec.ts --timeout=5000",
        "unit-usertypes-tests": "mocha -r ts-node/register tests/unit/usertypes/**/*.spec.ts",
        "unit-schemas-tests": "mocha -r ts-node/register tests/unit/schemas/**/*.spec.ts",
        "integration-infrastructure-tests": "nyc mocha -r ts-node/register tests/integration/infrastructure/**/*.spec.ts --timeout 7500 --exit",
        "integration-db-readmodel-tests": "nyc mocha -r ts-node/register tests/integration/db/readmodels/**/*.spec.ts --timeout 15000 --exit",
        "integration-db-writemodel-tests": "nyc mocha -r ts-node/register tests/integration/db/writemodels/**/*.spec.ts --timeout 15000 --exit",
        "integration-user-default-routes": "nyc mocha -r ts-node/register tests/integration/user/user.routes.spec.ts --timeout 60000 --exit",
        "integration-client-default-routes": "nyc mocha -r ts-node/register tests/integration/user/client.routes.spec.ts --timeout 60000 --exit",
        "integration-customer-default-routes": "nyc mocha -r ts-node/register tests/integration/user/customer.routes.spec.ts --timeout 60000 --exit",
        "integration-redis-local-server": "mocha -r ts-node/register tests/integration/redis/**/*.spec.ts --timeout 60000 --exit",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/index.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\"",
        "delete-dist": "rimraf -rf ./dist/"
    },
